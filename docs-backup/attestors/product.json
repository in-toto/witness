{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "DigestSet": {
      "additionalProperties": {
        "type": "string"
      },
      "type": "object"
    },
    "Product": {
      "properties": {
        "mime_type": {
          "type": "string"
        },
        "digest": {
          "$ref": "#/$defs/DigestSet"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "mime_type",
        "digest"
      ]
    }
  },
  "properties": {
    "Products": {
      "additionalProperties": {
        "$ref": "#/$defs/Product"
      },
      "type": "object"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "required": [
    "Products"
  ]
}
 